    "dependency-cruiser": "^17.2.0",
    "eslint": "^9.0.0",
    "jsdom": "^26.0.1",
    "prettier": "^3.0.0",
    "turbo": "^1.10.12",
    "typescript": "^5.0.0",
    "vite": "^6.4.1",
    "vitest": "^4.0.8"
  },
  "packageManager": "npm@10.0.0",
